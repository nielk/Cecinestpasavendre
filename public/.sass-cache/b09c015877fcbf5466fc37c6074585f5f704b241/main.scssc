@import "colors", "typo", "layout", "navigation", "slider";


/* page contribution styles */

.error {
  color: red;
}  
.sucess {
  color: green;
}  

form {
  width: 70%;
  margin: 0 auto;
}

.choses section {
/*  padding-top: 5%;
  padding-bottom: 5%;*/

/*  @media screen and (min-width: 480px) {
		padding-left: 10%;
		padding-right: 10%;
	}

	@media screen and (min-width: 720px) {
		padding-left: 30%;
		padding-right: 30%;
	}*/
}

.choses:nth-child(odd) section {
	text-align: left;
}

.choses:nth-child(even) section {
	text-align: right;
}

#menu-contrib {
  
  @media screen and (orientation: portrait) {
  	position: fixed;
  }
  position: fixed;
  @media screen and (orientation: landscape) and (max-width: 768px) {
  	position: absolute;
  }

  z-index: 1000;
  width: 100%;
  height: 100px;
  background: blue;
  background-color: #f0edde;
  background-image: linear-gradient(138deg, #f0edde 80%, #f0edde 80%, #ece8da 80%, #f0edde 100%);
}

#button-form-toggle {
  margin: 0 auto;
  width: 400px;
  margin-top: 20px;
  font-family: 'Holmes';
  font-size: 2em;

  /*
  border-radius: 45px;
  width: 80px;
  height: 80px;
  line-height: 80px;
  background-color: rgba(0,0,0,0.6);*/
  text-align: center;
  text-decoration: underline;
  color: rgb(0,0,0);
  cursor: pointer;

  	@media screen and (min-width: 480px) {
		font-size: 2em;
	}

	@media screen and (min-width: 720px) {
		font-size: 3em;
	}
}

#form-container {
  padding-top: 100px;
  height: 100%;
  background-color: rgb(54, 91, 99);
}

.chose {
  width: 180px;
  height: 180px;

  @media screen and (min-width: 480px) {
		width: 200px;
  		height: 200px;
	}
  @media screen and (min-width: 768px) {
		width: 200px;
  		height: 200px;
  }
}

.odd {
  margin: 0 10px;
  border: 1px solid #fff;
  display: inline-block;
  vertical-align: middle;
  text-align: left;
  float: center;
  	max-width: 350px;
  	min-width: 200px;
  @media screen and (min-width: 768px) {
  	max-width: 500px;
  	float: left;
  	min-width: 200px;
  }
}

.even {
  margin: 0 10px;
  border: 1px solid #fff;
  display: inline-block;
  vertical-align: middle;
  text-align: left;
  float: center;
  	max-width: 350px;
  	min-width: 200px;
  @media screen and (min-width: 768px) {
  	max-width: 500px;
  	float: right;
  	min-width: 200px;
  }
}

.container-chose {
	text-align: center;
	@media screen and (min-width: 1024px) {
  	  width: 80%;
  	  margin-left: 10%;
  }
}

/* #C9BD9D", "#BD4B57", "#403859", "#A63A59", "#92C0A9", "#C68847", "#DDBF74", "#365B63" */

.choses:nth-child(1) {
  margin-top: 100px;
}

.choses:nth-child(8n+1) {
  background-color: #C9BD9D;
}

.choses:nth-child(8n+2) {
  background-color: #BD4B57;
}

.choses:nth-child(8n+3) {
  background-color: #403859;
}

.choses:nth-child(8n+4) {
  background-color: #A63A59;
}

.choses:nth-child(8n+5) {
  background-color: #92C0A9;
}

.choses:nth-child(8n+6) {
  background-color: #C68847;
}

.choses:nth-child(8n+7) {
  background-color: #DDBF74;
}

.choses:nth-child(8n+8) {
  background-color: #365B63;
}

